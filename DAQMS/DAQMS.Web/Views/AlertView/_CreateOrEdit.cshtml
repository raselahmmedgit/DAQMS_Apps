@model  DAQMS.DomainViewModel.AlertHistoryViewModel

<div class="row-fluid form-horizontal">
    <div class="row-fluid WrapperFluid">
        <div class="span6">
            <div class="control-group">
                @Html.LabelFor(model => model.CompanyId, new { @class = "control-label" })
                <div class="controls">
                    @Html.HiddenFor(model => model.CompanyId)
                    @Html.TextBoxFor(model => model.CompanyName, new { @class = "form-control" })
                </div>
            </div>
        </div>
        <div class="span6">
            <div class="control-group">
                @Html.LabelFor(model => model.ProjectId, new { @class = "control-label" })
                <div class="controls">
                    @Html.HiddenFor(model => model.ProjectId)
                    @Html.TextBoxFor(model => model.ProjectName, new { @class = "form-control" })
                </div>
            </div>
        </div>
    </div>
    <div class="row-fluid WrapperFluid">
        <div class="span6">
            <div class="control-group">
                @Html.LabelFor(model => model.DeviceID, new { @class = "control-label" })
                <div class="controls">
                    @Html.TextBoxFor(model => model.DeviceID, new { @class = "form-control" })
                </div>
            </div>
        </div>
        <div class="span6">
            <div class="control-group">
                @Html.LabelFor(model => model.AlertType, new { @class = "control-label" })
                <div class="controls">
                    @Html.TextBoxFor(model => model.AlertType, new { @class = "form-control " })
                </div>
            </div>
        </div>
    </div>
    <div class="row-fluid WrapperFluid">
        <div class="span6">
            <div class="control-group">
                @Html.LabelFor(model => model.GenerateTimestamp, new { @class = "control-label" })
                <div class="controls">
                    @Html.TextBoxFor(model => model.GenerateTimestamp, new { @class = "form-control" })
                </div>
            </div>
        </div>
        <div class="span6">
            <div class="control-group">
                @Html.LabelFor(model => model.IsNew, new { @class = "control-label" })
                <div class="controls">
                    @Html.TextBoxFor(model => model.IsNew, new { @class = "form-control " })
                </div>
            </div>
        </div>
    </div>
    <div class="row-fluid WrapperFluid">
        <div class="span6">
            <div class="control-group">
                @Html.LabelFor(model => model.AlertMsg, new { @class = "control-label" })
                <div class="controls">
                    @Html.TextAreaFor(model => model.AlertMsg, new { @class = "form-control" })
                </div>
            </div>
        </div>
        <div class="span6">
        </div>
    </div>
</div>




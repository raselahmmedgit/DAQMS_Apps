<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="author" content="@Html.RenderApplicationMetaAuthor()">
    <meta name="keywords" content="@Html.RenderApplicationMetaKeywords()">
    <meta name="description" content="@Html.RenderApplicationMetaDescription()">
    <meta name="robots" content="index,follow" />
    <meta name="googlebot" content="index,follow" />
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>@Html.RenderApplicationTitle() | @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/themes/base/all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-2.2.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.11.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <!--Theme-->
    @Html.Partial("_ThemePartial")
    <script src="@Url.Content("~/Scripts/App/App.js")" type="text/javascript"></script>
</head>
<body class="hold-transition login-page">
    @RenderBody() 
    <script type="text/javascript">
        $(function () {
            App.init();
        });
    </script>
</body>
</html>